{
  "collectionName": "components_shared_socials",
  "info": {
    "displayName": "Social",
    "icon": "twitter",
    "description": ""
  },
  "options": {},
  "attributes": {
    "name": {
      "type": "string",
      "required": true
    },
    "url": {
      "type": "string",
      "required": true,
      "regex": "[(http(s)?):\\/\\/(www\\.)?a-zA-Z0-9@:%._\\+~#=]{2,256}\\.[a-z]{2,6}\\b([-a-zA-Z0-9@:%_\\+.~#?&//=]*)"
    },
    "icon": {
      "type": "customField",
      "required": true,
      "customField": "plugin::strapi-plugin-iconhub.iconhub"
    }
  }
}
